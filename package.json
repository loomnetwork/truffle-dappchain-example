{
  "name": "truffle-dappchain-example",
  "description": "Simple example of Truffle interacting with Loom DappChain",
  "scripts": {
    "truffle": "truffle",
    "deploy:reset": "truffle deploy --reset --network loom_dapp_chain",
    "deploy": "truffle deploy --network loom_dapp_chain",
    "test": "truffle test --network loom_dapp_chain",
    "serve": "webpack-dev-server --hot --content-base ./dist"
  },
  "dependencies": {
    "loom-js": "^1.14.0",
    "loom-truffle-provider": "^0.4.0",
    "truffle": "^4.1.13"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "regenerator-runtime": "^0.12.0",
    "web3": "^1.0.0-beta.34",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
