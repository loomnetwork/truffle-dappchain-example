{
  "name": "truffle-dappchain-example",
  "description": "Simple example of Truffle interacting with Loom DappChain",
  "scripts": {
    "truffle": "truffle",
    "deploy:reset": "truffle deploy --reset --network loom_dapp_chain",
    "deploy": "truffle deploy --network loom_dapp_chain",
    "test": "truffle test --network loom_dapp_chain",
    "serve": "webpack-dev-server --hot --content-base ./dist"
  },
  "dependencies": {
    "loom-js": "^1.20.0",
    "loom-truffle-provider": "^0.6.0",
    "truffle": "^4.1.14"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.55",
    "@babel/runtime": "^7.0.0-beta.46",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.0-beta.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.3.1",
    "regenerator-runtime": "^0.11.1",
    "request-promise": "^4.2.2",
    "web3": "^1.0.0-beta.34",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
